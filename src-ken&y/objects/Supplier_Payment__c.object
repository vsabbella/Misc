    <validationRules>
        <fullName>BA_Currency_and_SI_Currency_match</fullName>
        <active>true</active>
        <description>Bank account selected for the payment must have a currency that matches the currencies on the invoices</description>
        <errorConditionFormula>Bank_Account__r.Currency__c  &lt;&gt;  Currency__c</errorConditionFormula>
        <errorMessage>The invoices and credit memos you select to pay must have the same company and currency as your disbursement bank account. You need to review and change your selections.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancelled_not_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) == &apos;Cancelled&apos; &amp;&amp; Finalized__c</errorConditionFormula>
        <errorDisplayField>Finalized__c</errorDisplayField>
        <errorMessage>You cannot Finalize a Supplier Payment that has been cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Cannot_Change</fullName>
        <active>true</active>
        <description>This rule requires the payment amount does not change once the status has moved past &quot;Unapproved&quot;</description>
        <errorConditionFormula>NOT( OR(ISPICKVAL( Status__c , &apos;Unapproved&apos; ),ISPICKVAL( Status__c , &apos;Cancelled&apos; ))) &amp;&amp; (ISCHANGED( Supplier__c  ) ||
ISCHANGED( Payment_Method_G__c  ) || 
ISCHANGED(  Bank_Account__c   ) || 
ISCHANGED(   Status_Reason__c     ) || 
ISCHANGED( Company__c   ))</errorConditionFormula>
        <errorMessage>You cannot change the payment details once the payment status has moved past &quot;Unapproved&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Void_and_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>Void__c &amp;&amp; !Finalized__c</errorConditionFormula>
        <errorMessage>This Supplier Payment must be Finalized in order to Void</errorMessage>
    </validationRules>
