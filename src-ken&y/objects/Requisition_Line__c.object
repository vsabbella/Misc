        <errorConditionFormula>OR(ISNULL(Purchasing_UOM__c),ISBLANK(Purchasing_UOM__c)) 
&amp;&amp; NOT(ISBLANK(Item__c)) &amp;&amp;

( IsPickval (Item__r.Source__c, &apos;Purchased&apos;) ||
IsPickval (Item__r.Source__c, &apos;Miscellaneous&apos;) ||
IsPickval (Item__r.Source__c, &apos;Expense&apos;) ||
