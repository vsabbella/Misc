(
Item__c != Default_Purchase_Agreement_Line__r.Item__c  ||
 Purchasing_UOM__c !=  Default_Purchase_Agreement_Line__r.Purchase_UOM__c
)</errorConditionFormula>
        <errorMessage>Item and Purchase UOM on Default Purchase Agreement Line must match with Requisition Line</errorMessage>
    </validationRules>
