    <validationRules>
        <fullName>Default_Bill_To_should_be_a_Bill_To</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Default_Bill_To_Customer__c)) &amp;&amp;  (Default_Bill_To_Customer__r.RecordType.Name != &quot;Bill To&quot; ||  Default_Bill_To_Customer__r.Parent_Customer__r.Id != Id)</errorConditionFormula>
        <errorMessage>The Default Bill To Customer must be related to Corporate Parent Customer and have a &apos;Bill To&apos; record type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Track_Install_Base_field</fullName>
        <active>true</active>
        <errorConditionFormula>Track_Install_Base__c = false</errorConditionFormula>
        <errorMessage>Track Install Base field cannot be unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Parent_Record_Type</fullName>
        <active>true</active>
        <description>Check to see if the Parent Record Type is valid</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Parent_Customer__c)),
OR(
AND(RecordType.DeveloperName = &apos;Ship_To&apos;, Parent_Customer__r.RecordType.DeveloperName &lt;&gt; &apos;Sold_To&apos;),
AND(RecordType.DeveloperName = &apos;Sold_To&apos;, Parent_Customer__r.RecordType.DeveloperName &lt;&gt; &apos;Bill_To&apos;), AND(RecordType.DeveloperName = &apos;Bill_To&apos;, Parent_Customer__r.RecordType.DeveloperName &lt;&gt; &apos;Corporate_Parent&apos;)
)
)</errorConditionFormula>
        <errorDisplayField>Parent_Customer__c</errorDisplayField>
        <errorMessage>Invalid Parent Record Type</errorMessage>
    </validationRules>
