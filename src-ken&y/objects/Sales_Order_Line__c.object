    <validationRules>
        <fullName>Company_must_match</fullName>
        <active>true</active>
        <description>Company must match header company.</description>
        <errorConditionFormula>Sales_Order__r.Company__c  !=  Company__c</errorConditionFormula>
        <errorMessage>Company must match Sales Order Company</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_information_on_Resale_orders</fullName>
        <active>true</active>
        <description>Sales order lines with resale fulfillment source should either have an End customer or End customer reference populated</description>
        <errorConditionFormula>AND(ISPICKVAL(Fulfillment_Source__c, &apos;Resale&apos;), ISBLANK( End_Customer__c), ISBLANK(End_Customer_Reference__c))</errorConditionFormula>
        <errorMessage>Sales Order Lines with Fulfillment source as Resale should have End Customer or End customer reference field populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_percent_cannot_be_negative</fullName>
        <active>true</active>
        <errorConditionFormula>Discount_Percent__c &lt; 0</errorConditionFormula>
        <errorMessage>Discount Percent cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Fulfillment_source_on_SO_Lines</fullName>
        <active>true</active>
        <description>Lock Fulfillment source on SO Lines that are not on Draft status</description>
        <errorConditionFormula>!ISPICKVAL(Status__c , &quot;Draft&quot;) &amp;&amp; ISCHANGED(Fulfillment_Source__c)</errorConditionFormula>
        <errorMessage>You can only change the Fulfillment Source on a Sales Order line with Draft status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_ordered_and_shipped_validation</fullName>
        <active>false</active>
        <description>Quantity shipped cannot be greater than quantity ordered</description>
        <errorConditionFormula>AND(
  OR(ISCHANGED( Quantity_Ordered__c ),
     ISCHANGED( Quantity_Allocated__c ),
     ISCHANGED( Quantity_Shipped__c )
  ),
  Quantity_Ordered__c &lt; (Quantity_Allocated__c + Quantity_Shipped__c) 
)</errorConditionFormula>
        <errorDisplayField>Quantity_Ordered__c</errorDisplayField>
        <errorMessage>Quantity Ordered cannot be less than the sum of Quantity Allocated and Quantity Shipped.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Order_Lines_are_locked</fullName>
        <active>true</active>
        <errorConditionFormula>Sales_Order__r.Released_to_3PL__c == true</errorConditionFormula>
        <errorMessage>Sales Order Lines cannot be modified once Released to 3PL on Sales Order is true.</errorMessage>
    </validationRules>
