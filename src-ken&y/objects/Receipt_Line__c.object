    <validationRules>
        <fullName>Cannot_add_Drop_Ship_PO_to_ASN_RD</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Receiving_Document__r.Type__c, &apos;Advanced Shipment Notice&apos;), ISPICKVAL(Purchase_Order__r.Type__c , &apos;Drop Ship&apos; ))</errorConditionFormula>
        <errorMessage>Cannot add a Drop Ship Purchase Order Line to Receiving Document of type ASN</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Order_Line_Status_Check</fullName>
        <active>true</active>
        <description>If the record is new then the Purchase Order Line status should be &apos;Open&apos;</description>
        <errorConditionFormula>ISNEW() &amp;&amp; 
Purchase_Order_Line__c != null &amp;&amp;
NOT( ISPICKVAL(Purchase_Order_Line__r.Status__c, &apos;Open&apos;)) &amp;&amp; 
NOT(ISPICKVAL(Receiving_Document__r.Type__c, &apos;Purchase Order Return&apos;)) &amp;&amp;
NOT(ISPICKVAL(Receiving_Document__r.Type__c, &apos;Logical Purchase Order Return&apos;)) &amp;&amp; 
NOT(ISPICKVAL(Receiving_Document__r.Type__c, &apos;In-Transit&apos;))</errorConditionFormula>
        <errorMessage>Purchase Order Line must be Status &apos;Open&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Purchase_Order_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; 
Purchase_Order__c != null &amp;&amp; 
NOT( ISPICKVAL(Purchase_Order__r.Status__c, &apos;Open&apos;)) &amp;&amp; 
NOT( ISPICKVAL(Receiving_Document__r.Type__c, &apos;Purchase Order Return&apos;)) &amp;&amp;
NOT(ISPICKVAL(Receiving_Document__r.Type__c, &apos;Logical Purchase Order Return&apos;)) &amp;&amp; 
NOT( ISPICKVAL(Receiving_Document__r.Type__c, &apos;In-Transit&apos;))</errorConditionFormula>
        <errorMessage>Purchase Order must be Status Open</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>match_company</fullName>
        <active>true</active>
        <description>Company must match Receiving Document Company</description>
        <errorConditionFormula>Company__c != null &amp;&amp; 
Receiving_Document__r.Company__c != null &amp;&amp; 
Company__c != Receiving_Document__r.Company__c</errorConditionFormula>
        <errorMessage>Company must match Receiving Document Company</errorMessage>
    </validationRules>
