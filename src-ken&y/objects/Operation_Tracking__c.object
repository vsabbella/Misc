    <validationRules>
        <fullName>Actual_start_end_date</fullName>
        <active>true</active>
        <description>Actual end date must be after actual start date</description>
        <errorConditionFormula>OR(AND(Actual_End_Date__c != null,Actual_Start_Date__c == null),Actual_End_Date__c &lt; Actual_Start_Date__c)</errorConditionFormula>
        <errorMessage>Actual end date must be after actual start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fixed_backflush</fullName>
        <active>true</active>
        <description>Fixed hours is only allowed for backflushed resources</description>
        <errorConditionFormula>AND(NOT(Backflush_Resource__c),Fixed_Hours__c)</errorConditionFormula>
        <errorMessage>Fixed hours is only allowed for backflushed resources</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_negative_hours</fullName>
        <active>true</active>
        <description>Hours consumed cannot be negative</description>
        <errorConditionFormula>Hours_Consumed__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Hours_Consumed__c</errorDisplayField>
        <errorMessage>Hours consumed cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_negative_resource_quantity</fullName>
        <active>true</active>
        <description>Resource quantity cannot be negative</description>
        <errorConditionFormula>Resource_Quantity__c &lt; 0</errorConditionFormula>
        <errorMessage>Resource quantity cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_negative_standard_hours</fullName>
        <active>true</active>
        <description>Standard hours cannot be negative</description>
        <errorConditionFormula>Standard_Hours__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Standard_Hours__c</errorDisplayField>
        <errorMessage>Standard hours cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_date_populated_if_end_date</fullName>
        <active>true</active>
        <description>Start date must be populated if end date is populated</description>
        <errorConditionFormula>AND(Start_Date__c == null,End_Date__c != null)</errorConditionFormula>
        <errorMessage>Start date must be populated if end date is populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_end_date</fullName>
        <active>true</active>
        <description>End date must be after start date</description>
        <errorConditionFormula>OR(AND(End_Date__c != null,Start_Date__c == null),End_Date__c &lt; Start_Date__c)</errorConditionFormula>
        <errorMessage>End date must be after start date</errorMessage>
    </validationRules>
