    <fields>
        <fullName>Percent_Complete__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   Actual_End_Date__c != null,
   1,
   IF(
      OR(
         NULLVALUE(Standard_Hours__c,0) = 0,
         NULLVALUE(Hours_Consumed__c,0) = 0
      ),
      0,
      IF(
         Fixed_Hours__c,
         Hours_Consumed__c / Standard_Hours__c,
         Hours_Consumed__c / (Standard_Hours__c * Work_Order__r.Order_Quantity__c)
      )
   )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Percent Complete</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
