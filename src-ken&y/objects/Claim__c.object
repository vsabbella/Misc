    <validationRules>
        <fullName>Cannot_Update_Amount_While_Updating</fullName>
        <active>true</active>
        <errorConditionFormula>AND(not(ISNEW()),ISCHANGED(Amount__c ),  NOT( ISPICKVAL(Source__c, &quot;Manual&quot;) ) )</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>You cannot update the amount on a non manual claim.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Amount_on_Related_Claims</fullName>
        <active>true</active>
        <errorConditionFormula>AND(not(ISNEW()),ISCHANGED(Amount__c ), NOT( ISBLANK( Related_Claim__c ) ))</errorConditionFormula>
        <errorMessage>Cannot update amount on claim created from split</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_enter_a_zero_or_negative_amount</fullName>
        <active>true</active>
        <description>You can&apos;t enter a zero or negative amount for a manual claim.</description>
        <errorConditionFormula>AND(ISPICKVAL(Source__c, &quot;Manual&quot;) , Amount__c &lt;= 0)</errorConditionFormula>
        <errorMessage>You can&apos;t enter a zero or negative amount for a manual claim.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_cancelled_claims</fullName>
        <active>true</active>
        <description>User should not able to change the status of cancelled claims</description>
        <errorConditionFormula>AND(ISCHANGED(Status__c),ISPICKVAL(PRIORVALUE(Status__c) ,&apos;Cancelled&apos;))</errorConditionFormula>
        <errorMessage>Cancelled claim&apos;s status can&apos;t be updated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_Update_ClaimDate_on_Related_Claims</fullName>
        <active>true</active>
        <errorConditionFormula>AND(not(ISNEW()),ISCHANGED( Claim_Date__c  ), NOT( ISBLANK( Related_Claim__c ) ))</errorConditionFormula>
        <errorMessage>Cannot update claim date on claim created from split</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_enter_negative_amount_for_deduction</fullName>
        <active>true</active>
        <description>You can&apos;t enter a zero or negative amount for an invoice or general deduction claim.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Source__c, &quot;General Deduction&quot;), ISPICKVAL(Source__c, &quot;Invoice Deduction&quot;)) , Amount__c &lt;= 0)</errorConditionFormula>
        <errorMessage>You can&apos;t enter a zero or negative amount for an invoice or general deduction claim.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Claims_without_a_settlement_method</fullName>
        <active>true</active>
        <description>Claims cannot be closed without a settlement method.</description>
        <errorConditionFormula>AND( 
		ISPICKVAL(Status__c,&apos;Closed&apos;), 
		OR( 
		ISBLANK(TEXT(Settlement_Method__c)), 
		ISNULL(TEXT(Settlement_Method__c)) 
		)  
		)</errorConditionFormula>
        <errorMessage>Please populate settlement method while closing claims.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Line_Amount_more_than_Claim_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>if( Total_Line_Amount__c != 0, 
    if(Amount__c &lt; 0, 
       if(Amount__c &gt; Total_Line_Amount__c, true, false),
    if(Amount__c &lt; Total_Line_Amount__c, true, false)), false)</errorConditionFormula>
        <errorMessage>The Total Line Amount is exceeding the Amount on Claim</errorMessage>
    </validationRules>
