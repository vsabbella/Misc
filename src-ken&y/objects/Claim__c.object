    <fields>
        <fullName>AR_Reason_Code__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Lookup to Reason Code.</description>
        <externalId>false</externalId>
        <label>Reason Code</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>(1 OR 2) AND 3</booleanFilter>
            <errorMessage>You can’t select a resolution code with a company different from the claim company.</errorMessage>
            <filterItems>
                <field>Reason_Code__c.Company__c</field>
                <operation>equals</operation>
                <value></value>
            </filterItems>
            <filterItems>
                <field>Reason_Code__c.Company__c</field>
                <operation>equals</operation>
