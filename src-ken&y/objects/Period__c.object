    <validationRules>
        <fullName>Check_Company_Information</fullName>
        <active>true</active>
        <description>Check if finaicial company is missing on the record</description>
        <errorConditionFormula>ISBLANK(  Company__c )</errorConditionFormula>
        <errorMessage>Please select a valid financial company</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_InterCompany_subledger_cutoff</fullName>
        <active>false</active>
        <description>Check InterCompany cutoff should always be same as period end.</description>
        <errorConditionFormula>InterCompany_Subledger_Cutoff__c      &lt;  Period_End__c</errorConditionFormula>
        <errorMessage>Check InterCompany cutoff should always be same as period end.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Intercompany_SubledgerCutoff</fullName>
        <active>true</active>
        <description>Check Intercompany Cutoff. If the cutoff is before period end then throw error.</description>
        <errorConditionFormula>InterCompany_Subledger_Cutoff__c &lt;  Period_End__c</errorConditionFormula>
        <errorMessage>Intercompany cutoff can not be before Period End.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Year_End_Date</fullName>
        <active>true</active>
        <description>Check if Year end date is less than Year start date</description>
        <errorConditionFormula>Year_End_Date__c &lt;= Year_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Year_End_Date__c</errorDisplayField>
        <errorMessage>Please enter a Year End Date which is greater than Year Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_on_Period_Start_Date</fullName>
        <active>false</active>
        <description>Check if the period start date is between year start date and year end date</description>
        <errorConditionFormula>!ISPICKVAL(Type__c, &apos;Zero&apos;) &amp;&amp;
(DATEVALUE(Period_Start__c) &lt; Year_Start_Date__c || 
DATEVALUE(Period_Start__c) &gt; Year_End_Date__c)</errorConditionFormula>
        <errorDisplayField>Period_Start__c</errorDisplayField>
        <errorMessage>Please enter the period start between the year start date and year end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_on_period_type_selection</fullName>
        <active>true</active>
        <description>For a given record only one check box for period type should be checked.</description>
        <errorConditionFormula>( Fiscal__c == true &amp;&amp; Corporate__c == true &amp;&amp; Tax__c ==true ) || 
( Fiscal__c == true &amp;&amp; Corporate__c == true &amp;&amp; Tax__c == false ) || 
( Fiscal__c == true &amp;&amp; Corporate__c == false &amp;&amp; Tax__c == true) || 
( Fiscal__c == false &amp;&amp; Corporate__c == true &amp;&amp; Tax__c == true)</errorConditionFormula>
        <errorMessage>The period can be only one period type for a record Corporate or Fiscal or Tax.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_on_the_status_selection</fullName>
        <active>true</active>
        <description>To perform the check on the status of the period. For a given record only one status at a time is valid.</description>
        <errorConditionFormula>( Closed__c = true &amp;&amp; Open__c == true &amp;&amp; Permanently_Closed__c == true) || 
( Closed__c = true &amp;&amp; Open__c == true &amp;&amp; Permanently_Closed__c == false) || 
( Closed__c = true &amp;&amp; Open__c == false&amp;&amp; Permanently_Closed__c == true) || 
( Closed__c = false &amp;&amp; Open__c == true &amp;&amp; Permanently_Closed__c == true)</errorConditionFormula>
        <errorMessage>The period can only be in one status at a time (Open or Closed or Permanently Closed)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fiscal_Corporate_Tax_Validation</fullName>
        <active>true</active>
        <description>The purpose of this rule is to identify if atleast one checkbox is selected</description>
        <errorConditionFormula>Corporate__c == false &amp;&amp; Fiscal__c == false &amp;&amp; Tax__c == false &amp;&amp; !ISPICKVAL(Type__c, &apos;Historical&apos;)</errorConditionFormula>
        <errorMessage>Please select at least one period type of Fiscal/Corporate/Tax</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fiscal_Year_Check</fullName>
        <active>true</active>
        <description>Fiscal Year check for the start and end dates</description>
        <errorConditionFormula>NOT(ISBLANK(FiscalYear__c)) 
&amp;&amp; 
NOT(ISPICKVAL(Type__c, &apos;Zero&apos;))
&amp;&amp;
(Period_Start__c &lt; FiscalYear__r.Fiscal_Year_Start_Date__c  
||
Period_End__c  &gt; FiscalYear__r.Fiscal_Year_End_Date__c)</errorConditionFormula>
        <errorMessage>Period start date and end date should be within the associated fiscal year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fiscal_Year_can_not_be_blank</fullName>
        <active>true</active>
        <description>Fiscal year can not be blank</description>
        <errorConditionFormula>ISBLANK( FiscalYear__c )</errorConditionFormula>
        <errorDisplayField>Fiscal_Year__c</errorDisplayField>
        <errorMessage>Fiscal year can not be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_status_Change_on_Permanently_Closed</fullName>
        <active>true</active>
        <description>Once the status of the record is Permanently Closed it can not be changed to Closed or Open.</description>
        <errorConditionFormula>(Permanently_Closed__c == false &amp;&amp; ISCHANGED( Permanently_Closed__c))</errorConditionFormula>
        <errorDisplayField>Permanently_Closed__c</errorDisplayField>
        <errorMessage>Status of the record is Permanently Closed and it can not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Open_Period_Validations</fullName>
        <active>true</active>
        <description>Once the period is opened, the period start date, period end date, period number, quarter number, year start date, year end date can not be modified.</description>
        <errorConditionFormula>(Open__c == true || Closed__c == true || Permanently_Closed__c == true) &amp;&amp; (
 ISCHANGED( Period_Start__c ) ||
 ISCHANGED ( Period_End__c ) ||
 ISCHANGED ( Year_Start_Date__c )||
 ISCHANGED( Year_End_Date__c )||
 ISCHANGED( Period_Number__c ) ||
 ISCHANGED( Quarter_Number__c )
)</errorConditionFormula>
        <errorMessage>Period Start, Period End, Year Start, Year End, Period Number, Quarter Number can not be modified when the period is Open.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Period_Cannot_Close_with_Open_Subledgers</fullName>
        <active>true</active>
        <errorConditionFormula>!ISPICKVAL(Type__c, &apos;Historical&apos;) 
&amp;&amp;
!ISPICKVAL(Type__c, &apos;Zero&apos;)
&amp;&amp;
!ISPICKVAL(Type__c, &apos;Period 13&apos;)
&amp;&amp;
OR(Closed__c, Permanently_Closed__c)
&amp;&amp;
OR( 
ISNULL( AP_Subledger_Closed__c ) ,
ISNULL( AR_Subledger_Closed__c ) ,
ISNULL( Inventory_Subledger_Closed__c ) ,
ISNULL( FA_Subledger_Closed__c ) ,
ISNULL( Shipment_Subledger_Closed__c ) ,
ISNULL( Receiving_Subledger_Closed__c ),
ISNULL( GL_Close__c )

)</errorConditionFormula>
        <errorDisplayField>Closed__c</errorDisplayField>
        <errorMessage>You cannot close this period with open areas.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Period_Close_check</fullName>
        <active>true</active>
        <errorConditionFormula>PRIORVALUE(Closed__c) == false &amp;&amp; !(ISCHANGED(Closed__c)) &amp;&amp; Closed__c == true</errorConditionFormula>
        <errorMessage>Period can not be modified when in closed state</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Period_End_Date_Validation</fullName>
        <active>false</active>
        <description>Check if the period end date is greater than period start date and it is between year start date and year end date</description>
        <errorConditionFormula>(DATEVALUE(Period_End__c) &lt; DATEVALUE(Period_Start__c)) ||
(DATEVALUE(Period_End__c) &lt; Year_Start_Date__c || 
DATEVALUE(Period_End__c) &gt; Year_End_Date__c)</errorConditionFormula>
        <errorDisplayField>Period_End__c</errorDisplayField>
        <errorMessage>Please enter valid Period end. Period End should be greater than Period Start and It should fall in between year start date and year end date</errorMessage>
    </validationRules>
