    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Cancelled_not_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) == &apos;Cancelled&apos; &amp;&amp; Finalized__c</errorConditionFormula>
        <errorDisplayField>Finalized__c</errorDisplayField>
        <errorMessage>You cannot Finalize a Journal Entry that has been cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Type_Check</fullName>
        <active>true</active>
        <description>If the selected transaction type is Manual then no document lookup should be populated</description>
        <errorConditionFormula>((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK(Shipment__c) )
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK(Customer_Invoice__c) )
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK(Customer_Receipt__c) )
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK(Receiving_Document__c))
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK(Supplier_Invoice__c))
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK(Supplier_Payment__c))
|| 
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK( Customer_Credit_Memo__c ))
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK( Fixed_Asset_Adjustment__c ))
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK( Fixed_Asset_Depreciation__c  ))
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK( Inventory_Change__c   ))
||
((ISPICKVAL( Type__c , &apos;Manual&apos;)) &amp;&amp; !ISBLANK( Work_Order__c  ))</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Manual Transaction Type should not be associated with any source document.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Check_Void_Cancel</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Total_Number_Of_Lines__c ) &amp;&amp; (  OR(ISPICKVAL( Status__c , &apos;Void&apos;),ISPICKVAL( Status__c , &apos;Cancelled&apos;)) )</errorConditionFormula>
        <errorMessage>You cannot add Journal Entry Line that has been cancelled or voided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Lines_And_Finalized_Check</fullName>
        <active>true</active>
        <description>Block if the Total Lines on the JE are &lt; 2 and record is being finalized.</description>
        <errorConditionFormula>Total_Number_Of_Lines__c &lt; 2 &amp;&amp;  Finalized__c == true &amp;&amp; !(ISPICKVAL( Type__c, &apos;Year End&apos;))</errorConditionFormula>
        <errorMessage>Journal Entry can not be finalized with 1 line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_date_is_not_in_period</fullName>
        <active>true</active>
        <errorConditionFormula>if(ISPICKVAL( Type__c , 
&apos;Year End&apos;) || ISPICKVAL( Type__c , 
&apos;Bank Transaction&apos;) || ISPICKVAL( Type__c , 
&apos;Year End Adjustment&apos;) ,
false,IF(ISBLANK(Period__c),false,if(Transaction_Date__c  &gt;=  Period__r.Period_Start__c   &amp;&amp;
Transaction_Date__c &lt; Period__r.Period_End__c ,false,true)))</errorConditionFormula>
        <errorMessage>Transaction date or Finalized date cannot be after JE Cutoff.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_End_Adjustment_Company</fullName>
        <active>true</active>
        <errorConditionFormula>Period__c != null  &amp;&amp; 
Company__c  &lt;&gt;  Period__r.Company__c</errorConditionFormula>
        <errorDisplayField>Period__c</errorDisplayField>
        <errorMessage>The company for the period must be the same as the company for the journal entry</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_End_Adjustment_Period</fullName>
        <active>true</active>
        <errorConditionFormula>Period__c  = null  &amp;&amp; ISPICKVAL(Type__c, &apos;Year End adjustment&apos;)</errorConditionFormula>
        <errorDisplayField>Period__c</errorDisplayField>
        <errorMessage>When the Type field is Year End Adjustment, a value in the Period field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_End_Adjustment_Period_type</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type__c, &apos;Year End adjustment&apos;) &amp;&amp; NOT(ISPICKVAL(Period__r.Type__c,&apos;Period 13&apos;))</errorConditionFormula>
        <errorDisplayField>Period__c</errorDisplayField>
        <errorMessage>When the Type field is Year End Adjustment, the Type field for the period must be Period 13.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_End_Dates</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(Type__c, &apos;Year End adjustment&apos;) &amp;&amp;( (Period__r.FiscalYear__r.Fiscal_Year_End_Date__c  - GL_Date_Time__c) * 86400  &lt;&gt; 1 ), 
ISPICKVAL(Type__c, &apos;Year End&apos;) &amp;&amp;( (Period__r.FiscalYear__r.Fiscal_Year_End_Date__c  -  GL_Date_Time__c ) * 86400  &lt;&gt; 1 ))

||

OR(ISPICKVAL(Type__c, &apos;Year End adjustment&apos;) &amp;&amp;( (Period__r.FiscalYear__r.Fiscal_Year_End_Date__c - Posting_Date__c) * 86400 &lt;&gt; 1 ), 
ISPICKVAL(Type__c, &apos;Year End&apos;) &amp;&amp;( (Period__r.FiscalYear__r.Fiscal_Year_End_Date__c - Posting_Date__c ) * 86400 &lt;&gt; 1 ))

||

OR(ISPICKVAL(Type__c, &apos;Year End adjustment&apos;) &amp;&amp;( (Period__r.FiscalYear__r.Fiscal_Year_End_Date__c - Transaction_Date__c) * 86400 &lt;&gt; 1 ), 
ISPICKVAL(Type__c, &apos;Year End&apos;) &amp;&amp;( (Period__r.FiscalYear__r.Fiscal_Year_End_Date__c - Transaction_Date__c ) * 86400 &lt;&gt; 1 ))</errorConditionFormula>
        <errorMessage> When the Type field value of a journal entry is Year End Adjustment or Year End, the transaction date, GL date, and posting date must be one second less than fiscal year end date. Save the journal entry before finalizing it.</errorMessage>
    </validationRules>
