    <validationRules>
        <fullName>Cannot_Edit_Purchase_Agreement</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED( Purchase_Agreement__c ) )</errorConditionFormula>
        <errorDisplayField>Purchase_Agreement__c</errorDisplayField>
        <errorMessage>Purchase Agreement cannot be updated on a Purchase Order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Address_Fields</fullName>
        <active>true</active>
        <description>Cannot update Address fields on a Drop Ship Purchase Order.</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISPICKVAL( Type__c , &apos;Drop Ship&apos;) ,  
 OR (ISCHANGED( Deliver_To_City__c ), 
     ISCHANGED( Deliver_To_Street__c ),
     ISCHANGED( Deliver_To_State__c ),
     ISCHANGED( Deliver_To_Country__c ),
     ISCHANGED( Deliver_To_Zip_Code__c ) 
    )
  )</errorConditionFormula>
        <errorMessage>Delivery Address Fields cannot be updated on a Drop Ship Purchase Order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Check</fullName>
        <active>true</active>
        <description>If status is closed you cannot edit any fields</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(PRIORVALUE(Status__c), &apos;Open&apos;)),
NOT(ISCHANGED(Status__c)),
(ISCHANGED(Approval_Status__c)),
NOT(ISCHANGED(Open_Purchase_Order_Lines__c)),
NOT(ISCHANGED(Purchase_Order_Value__c)),
NOT(ISCHANGED(Quantity_Released__c)),
NOT(ISCHANGED(Total_Number_Of_Lines__c)),
NOT(ISCHANGED(Total_Order_Weight__c)))</errorConditionFormula>
        <errorMessage>Purchase Order is Not Open. No Changes are allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Company_must_not_change</fullName>
        <active>true</active>
        <description>after creating a PO for a given company, do not allow the PO to change to another company</description>
        <errorConditionFormula>ISCHANGED(Company__c)</errorConditionFormula>
        <errorMessage>Once created for a Company, the PO must not change Company field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_date_should_be_greater_than_today</fullName>
        <active>true</active>
        <description>Need date should be greater than or equal to today.</description>
        <errorConditionFormula>Not(IsBlank(Need_Date__c)) &amp;&amp;
IsChanged(Need_Date__c) &amp;&amp;
Need_Date__c &lt; today()</errorConditionFormula>
        <errorDisplayField>Need_Date__c</errorDisplayField>
        <errorMessage>Need date should be greater than or equal to today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negative_Tolerance</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
   OR( Quantity_Tolerance_Over__c &lt; 0,
       Unit_Price_Tolerance_Over__c &lt; 0,
       Amount_Tolerance__c &lt; 0), 
TRUE, FALSE)</errorConditionFormula>
        <errorMessage>All tolerance percentages and amounts must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Void_and_Finalized</fullName>
        <active>false</active>
        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>This Receiving Document must be Finalized in order to Void</errorMessage>
    </validationRules>
