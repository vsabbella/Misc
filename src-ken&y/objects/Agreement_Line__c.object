        <errorConditionFormula>(ISNEW()||
ISCHANGED(Start_Date__c))
&amp;&amp;
(OR(
(DATEVALUE(Start_Date__c)) &lt; (DATEVALUE(Agreement__r.Start_Date__c)),
(DATEVALUE(Start_Date__c)) &gt; (DATEVALUE(Agreement__r.End_Date__c )))
)</errorConditionFormula>
        <errorMessage>Start date cannot be less than Agreement start date and cannot be more than Agreement End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_line_Of_approved_agreement</fullName>
        <active>true</active>
        <description>Agreement Lines cannot be changed after Agreement is Approved.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Agreement__r.Status__c ,&apos;Approved&apos;),
(Updatable__c = false),
NOT(LastModifiedDate &lt;&gt; LastModifiedDate),
OR(
NOT(ISCHANGED(Updatable__c))),
OR(
NOT(ISCHANGED(Sold_Quantity__c))),
OR(
NOT(ISCHANGED(Remaining_Quantity__c)))
)</errorConditionFormula>
        <errorMessage>Agreement lines cannot be changed after agreement is approved.Please use agreement revision .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_add_rental_agreement_line</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISPICKVAL(Price_Type__c, &apos;Recurring&apos;) ,
 NOT(ISPICKVAL(Product__r.Item__r.Fulfillment_Source__c, &apos;Inventory&apos;))
)</errorConditionFormula>
        <errorMessage>You cannot create recurring agreement lines for items with a non-inventory fulfillment source</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_product_agreementline</fullName>
        <active>true</active>
        <description>Product is Required</description>
        <errorConditionFormula>ISNULL(Product__c)||ISBLANK(Product__c)</errorConditionFormula>
        <errorMessage>Product is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>cannot_add_line_with_sale_and_rent_UOM</fullName>
        <active>true</active>
        <description>Validation rule to prevent users from creating agreement lines of type sale with pricing uom &apos;rental&apos; ie., uom of type &apos;Time&apos;</description>
        <errorConditionFormula>AND( ISPICKVAL(Price_Type__c, &apos;Sales&apos;),
 ISPICKVAL( Pricing_UOM__r.Type__c , &apos;Time&apos;) )</errorConditionFormula>
        <errorMessage>You cannot create sales agreement lines for items with a rental unit of measure</errorMessage>
    </validationRules>
</CustomObject>
