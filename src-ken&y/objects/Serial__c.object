    <validationRules>
        <fullName>Verify_Unlink_Asset_To_Serial</fullName>
        <active>true</active>
        <description>The user can  remove the link of the asset to the serial number only if the item SKID is not already flagged as a rental.</description>
        <errorConditionFormula>AND( ISCHANGED(Fixed_Asset__c),
     
      SKID__r.Rental__c  = TRUE ,
 PRIORVALUE(Fixed_Asset__c) !=NULL
  
)</errorConditionFormula>
        <errorMessage>This serial number  cannot be detached from the Asset as the item SKID is already flagged as rental.</errorMessage>
    </validationRules>
