    <validationRules>
        <fullName>NonShippable_Flag_checked_for_Kit</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Source__c , &apos;Kit&apos;) &amp;&amp;  Non_Shippable__c = FALSE</errorConditionFormula>
        <errorMessage>You must select Non Shippable on the Order Management tab for an item with a source set to Kit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Inventory_Flag_checked</fullName>
        <active>true</active>
        <description>Non Inventory should be checked for Miscellaneous and Expense Items.</description>
        <errorConditionFormula>(ISPICKVAL(Source__c , &apos;Miscellaneous&apos;)  || ISPICKVAL(Source__c , &apos;Expense&apos;))  &amp;&amp;  Non_Inventory__c  = False</errorConditionFormula>
        <errorMessage>Non Inventory should be checked for Miscellaneous and Expense Items</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Range_Validation</fullName>
        <active>true</active>
        <description>Check for the Shrinkage that it is between 0 and 100.</description>
        <errorConditionFormula>OR(Shrinkage__c &lt;0,Shrinkage__c &gt;100)</errorConditionFormula>
        <errorDisplayField>Shrinkage__c</errorDisplayField>
        <errorMessage>Shrinkage should be between 0 and 100.</errorMessage>
    </validationRules>
