    <validationRules>
        <fullName>BA_Not_Allowed_for_Auto_Reversal_JE</fullName>
        <active>true</active>
        <description>Validation rule that throws an error if Bank account on JEL is populated while the checkbox for Auto-reversal is set to true.</description>
        <errorConditionFormula>AND(Journal_Entry__r.Auto_Reverse__c, Bank_Account__c != null)</errorConditionFormula>
        <errorMessage>You can&apos;t enter a bank account on journal entry lines when the Auto Reverse field is selected. You must remove bank account values on all lines before you can select the Auto Reverse field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Transaction_Currency_should_be_same</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISBLANK(Bank_Transaction__c)) &amp;&amp;  Bank_Transaction__r.Currency__r.ISO_Code__c != Journal_Entry__r.Currency__r.ISO_Code__c</errorConditionFormula>
        <errorMessage>The transaction currency on the bank transaction and journal entry must be same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Transaction_should_be_finalized</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISBLANK(Bank_Transaction__c)) &amp;&amp; ( Bank_Transaction__r.Finalized__c == false ||  Bank_Transaction__r.Bank_Transaction_Group__r.Void__c == true)</errorConditionFormula>
        <errorMessage>Please select bank transactions that are finalized and not voided</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_account_have_same_currency</fullName>
        <active>true</active>
        <description>Bank account must have the same currency as the Journal Entry</description>
        <errorConditionFormula>NOT(ISBLANK( Bank_Account__c )) &amp;&amp; Currency__c != Bank_Account__r.Currency__r.ISO_Code__c</errorConditionFormula>
        <errorMessage>You can&apos;t save this journal entry because the currency is different from the bank account currency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CR_DR_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_CR__c != 0 &amp;&amp; Amount_DR__c != 0</errorConditionFormula>
        <errorMessage>Amount Debit and Amount Credit both can not be specified on the same journal entry line. Please create separate lines for Credit and Debit.</errorMessage>
    </validationRules>
</CustomObject>
