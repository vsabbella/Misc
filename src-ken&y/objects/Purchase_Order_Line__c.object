    <validationRules>
        <fullName>Cannot_Edit_Order_Qty_less_Qty_Received</fullName>
        <active>true</active>
        <errorConditionFormula>AND(((Order_Quantity__c+Overage_Quantity__c ) &lt;  (Quantity_Received__c -  Quantity_Returned_to_Supplier__c )),  ISCHANGED(Order_Quantity__c) )</errorConditionFormula>
        <errorMessage>Cannot reduce the Purchase Order Quantity less than the Quantity Received</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_edit_order_qty_less_qty_invoiced</fullName>
        <active>true</active>
        <description>Validation rule to throw an error when the order qty goes below the qty invoiced</description>
        <errorConditionFormula>AND (ISCHANGED(Order_Quantity__c) , Order_Quantity__c &lt; ( Quantity_Matched__c ))</errorConditionFormula>
        <errorMessage>Cannot reduce the order quantity less than the quantity invoiced</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_Back_To_Back_POL</fullName>
        <active>true</active>
        <description>Cannot update a back to back Purchase Order Line.</description>
        <errorConditionFormula>ISPICKVAL( Purchase_Order__r.Type__c , &apos;Standard&apos;) &amp;&amp; 
ISPICKVAL(  Type__c , &apos;Back To Back&apos;) &amp;&amp;    

NOT(ISBlank( Sales_Order_Line__c ))   &amp;&amp; 

( 

ISCHANGED( Order_Quantity__c )|| 

ISCHANGED( Unit_Price__c )|| 

ISCHANGED( Need_Date__c )||

ISCHANGED( Delivery_Facility__c )||

ISCHANGED( Delivery_Location__c) 

)</errorConditionFormula>
        <errorMessage>Back To Back Purchase Order Line cannot be updated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_Drop_Ship_POL</fullName>
        <active>true</active>
        <description>Cannot update a drop ship Purchase Order Line.</description>
        <errorConditionFormula>ISPICKVAL( Purchase_Order__r.Type__c , &apos;Drop Ship&apos;) &amp;&amp;    

NOT(ISBlank( Sales_Order_Line__c ))   &amp;&amp; 

( 

ISCHANGED( Order_Quantity__c )|| 

ISCHANGED( Unit_Price__c )|| 

ISCHANGED( Need_Date__c )||

ISCHANGED( Delivery_Facility__c )||

ISCHANGED( Delivery_Location__c) 

)</errorConditionFormula>
        <errorMessage>Drop Ship Purchase Order Line cannot be updated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_invoice_matching</fullName>
        <active>true</active>
        <description>Validation rule to prevent changing the invoice matching on an approved purchase order line</description>
        <errorConditionFormula>AND( ISCHANGED( Invoice_Matching__c ) ,Approved__c )</errorConditionFormula>
        <errorMessage>Cannot update invoice matching field on an approved purchase order line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_line_update_validation</fullName>
        <active>true</active>
        <description>Cannot update a closed purchase order line</description>
        <errorConditionFormula>AND( NOT(ISCHANGED( Status__c )), ISPICKVAL(Status__c,&quot;Closed&quot;),NOT(Consigned__c),OR( ISCHANGED( Item__c ),  ISCHANGED( Description__c ) , ISCHANGED( Need_Date__c ),  ISCHANGED( Order_Quantity__c ),  ISCHANGED( Unit_Price__c ) ) )</errorConditionFormula>
        <errorMessage>You cannot update a closed line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_line_update_validation_header</fullName>
        <active>true</active>
        <description>Check the header status and if it is closed then do not allow updating the line</description>
        <errorConditionFormula>AND( ISPICKVAL(Purchase_Order__r.Status__c,&quot;Closed&quot;),NOT(Consigned__c),OR( ISCHANGED( Item__c ),  ISCHANGED( Description__c ) , ISCHANGED( Need_Date__c ),  ISCHANGED( Order_Quantity__c ),  ISCHANGED( Unit_Price__c ) ) )</errorConditionFormula>
        <errorMessage>Cannot update purchase order line as the Purchase Order is closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Matching_for_Non_Inventory_POL</fullName>
        <active>true</active>
        <description>Invoice Matching for Non Inventory Purchase Order Line can only be Two Way.</description>
        <errorConditionFormula>ISPICKVAL( Purchase_Order__r.Type__c , &apos;Drop Ship&apos;) &amp;&amp; Item__r.Non_Inventory__c  = true  &amp;&amp;
NOT(ISPICKVAL(Invoice_Matching__c, &apos;Two Way&apos;))</errorConditionFormula>
        <errorMessage>Invoice Matching for Non Inventory Purchase Order Line can only be Two Way.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_Cross_Reference</fullName>
        <active>false</active>
        <description>The Item and Supplier from the Item Cross Reference must match the Item and Supplier on this Purchase Order Line.</description>
        <errorConditionFormula>false</errorConditionFormula>
        <errorMessage>The Item and Supplier from the Item Cross Reference must match the Item and Supplier on this Purchase Order Line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_cannot_change_on_an_approved_POL</fullName>
        <active>true</active>
        <description>Item cannot be changed on an approved Purchase Order Line.</description>
        <errorConditionFormula>Approved__c &amp;&amp; ISCHANGED(Item__c)</errorConditionFormula>
        <errorDisplayField>Item__c</errorDisplayField>
        <errorMessage>Item cannot be changed on an approved Purchase Order Line.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_Date_required</fullName>
        <active>true</active>
        <description>ensure that the need date on a line is set</description>
        <errorConditionFormula>ISNULL(Need_Date__c)</errorConditionFormula>
        <errorMessage>The Need Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_date_should_be_greater_than_today</fullName>
        <active>true</active>
        <description>Need date should be greater than or equal to today.</description>
        <errorConditionFormula>Not(IsBlank(Need_Date__c)) &amp;&amp; 
IsChanged(Need_Date__c) &amp;&amp; 
Need_Date__c &lt; today()</errorConditionFormula>
        <errorDisplayField>Need_Date__c</errorDisplayField>
        <errorMessage>Need date should be greater than or equal to today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_quantity_required</fullName>
        <active>true</active>
        <description>Order quantity is required</description>
        <errorConditionFormula>IF(AND (OR(ISCHANGED( Order_Quantity__c ),ISNEW()),OR(ISNULL(Order_Quantity__c ),Order_Quantity__c  &lt;= 0)) ,true,false)</errorConditionFormula>
        <errorDisplayField>Order_Quantity__c</errorDisplayField>
        <errorMessage>Order quantity should be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Overage_Percentage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNULL(Overage_Percentage__c )), OR(Overage_Percentage__c &lt;0,Overage_Percentage__c &gt;1))</errorConditionFormula>
        <errorMessage>Overage Percentage must be greater than or equal to 0% and less than or equal to 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unit_price_required</fullName>
        <active>true</active>
        <description>Unit_price_required</description>
        <errorConditionFormula>IF(AND (OR(ISCHANGED(Unit_Price__c),ISNEW()),OR(ISNULL(Unit_Price__c),Unit_Price__c &lt; 0)) ,true,false)</errorConditionFormula>
        <errorDisplayField>Unit_Price__c</errorDisplayField>
        <errorMessage>Unit price cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Supplier</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
   AND(NOT(ISNULL(Supplier__c)),NOT(ISBLANK(Supplier__c)),
   ISPICKVAL(Status__c, &apos;Open&apos;), 
   OR(Supplier__r.RecordType.DeveloperName != &apos;Purchase_From&apos;,
      Supplier__r.Hold_Purchasing__c)),
TRUE,FALSE)</errorConditionFormula>
        <errorMessage>Cannot update Purchase Order Lines when Supplier is not of type Purchase From or on Purchasing Hold.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>match_company</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Item__r.Company__c)) &amp;&amp;
NOT(ISNULL(Item__r.Company__c)) &amp;&amp; 
Purchase_Order__r.Company__c  !=  Item__r.Company__c  &amp;&amp; Indirect__c==false</errorConditionFormula>
        <errorDisplayField>Company__c</errorDisplayField>
        <errorMessage>Purchase Order Line Company must match Purchase Order Company</errorMessage>
    </validationRules>
