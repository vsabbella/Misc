    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Bill_to_top_parent_equals_CR_top_parent</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Customer__c ) )   &amp;&amp; ( IF( Customer_Receipt__r.Customer__r.RecordType.DeveloperName  = &quot;Corporate_Parent&quot;,  IF(  Customer__r.Parent_Customer__c !=  Customer_Receipt__r.Customer__c , true, false) ,  IF(Customer_Receipt__r.Customer__r.RecordType.DeveloperName  = &quot;Bill_To&quot;,  IF(Customer__c !=   Customer_Receipt__r.Customer__c, true, false) , false) )  )</errorConditionFormula>
        <errorMessage>The Bill To customer must belong to the customer on the receipt</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Reason_Code_on_General_Deduction</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &apos;Invoice Deduction&apos;)|| ISPICKVAL(Type__c, &apos;General Deduction&apos;)) &amp;&amp;   ISBLANK(Reason_Code__c)</errorConditionFormula>
        <errorMessage>The Reason Code for a deduction is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Reason_Code_on_Write_Off_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &apos;Writeoff&apos;)) &amp;&amp;   ISBLANK(Reason_Code__c)</errorConditionFormula>
        <errorMessage>The Reason Code is required. Enter a Reason Code to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SO_should_be_associated_with_prepayment</fullName>
        <active>false</active>
        <description>Sales order should be associated with prepayment type of CR line.
Also, the &apos;bill to&apos; on sales order should match with customer on CR.</description>
        <errorConditionFormula>OR(
   AND(NOT(ISBLANK(Sales_Order__c)),
   ISPICKVAL(Type__c,&apos;Overpayment&apos;)),
   IF(AND(NOT(ISBLANK(Sales_Order__c)), ISPICKVAL(Type__c,&apos;Prepayment&apos;)),
   IF(OR(
          ISPICKVAL(Sales_Order__r.Status__c, &apos;Draft&apos;),
          ISPICKVAL(Sales_Order__r.Status__c, &apos;Open&apos;)
          )
      ,IF(OR(Sales_Order__r.Bill_to__c == Customer_Receipt__r.Customer__c,Sales_Order__r.Bill_to__r.Parent_Customer__c == Customer_Receipt__r.Customer__c),false,true),true),false))</errorConditionFormula>
        <errorMessage>Please select &quot;Type&quot; as &quot;Prepayment&quot; and &quot;Open&quot; sales orders associated to related customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sold_To_Parent_equals_Bill_To</fullName>
        <active>true</active>
        <errorConditionFormula>not(isblank( Sold_To_Customer__c )) &amp;&amp; Sold_To_Customer__r.Parent_Customer__c !=  Customer__c</errorConditionFormula>
        <errorMessage>Sold To&apos;s Parent Customer must equal Bill To</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Write_Off_Amount_Cannot_be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &apos;Writeoff&apos;) &amp;&amp;   NOT(ISBLANK(Amount__c)) &amp;&amp; Amount__c &lt; 0)</errorConditionFormula>
        <errorMessage>The Write Off Amount on an invoice cannot be a negative number. Enter a positive number to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wrong_Reason_Code_on_General_Deduction</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &apos;Invoice Deduction&apos;)|| ISPICKVAL(Type__c, &apos;General Deduction&apos;)) &amp;&amp;   NOT(ISBLANK(Reason_Code__c)) &amp;&amp; NOT(ISPICKVAL(Reason_Code__r.Type__c, &apos;Deductions&apos;))</errorConditionFormula>
        <errorMessage>The Reason Code for a Receipt Line of  Type General or Invoice Deduction should be Deductions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wrong_Reason_Code_on_Write_Off_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &apos;Writeoff&apos;) &amp;&amp;  NOT(ISBLANK(Reason_Code__c))   &amp;&amp; NOT(ISPICKVAL(Reason_Code__r.Type__c, &apos;Resolution&apos;)))</errorConditionFormula>
        <errorMessage>The Reason Code for a receipt line of type Write Off should be Resolution.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wrong_record_type_for_Sold_To_Customer</fullName>
        <active>true</active>
        <errorConditionFormula>not(isblank( Sold_To_Customer__c )) &amp;&amp; Sold_To_Customer__r.RecordType.DeveloperName != &quot;Sold_To&quot;</errorConditionFormula>
        <errorMessage>A Sold To Customer must have a Sold To Record Type</errorMessage>
    </validationRules>
</CustomObject>
