    <validationRules>
        <fullName>Amount_due_cannot_be_negative</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_Due__c &lt;0</errorConditionFormula>
        <errorMessage>Amount due cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancelled_not_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) == &apos;Cancelled&apos; &amp;&amp; Finalized__c</errorConditionFormula>
        <errorDisplayField>Finalized__c</errorDisplayField>
        <errorMessage>You cannot Finalize a Supplier Invoice that has been cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_be_Approved_for_Pmt_w_Exception</fullName>
        <active>true</active>
        <description>Invoices with an exception cannot be approved for payment</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &apos;Approved&apos;) &amp;&amp;  Has_Exceptions__c</errorConditionFormula>
        <errorDisplayField>Exception__c</errorDisplayField>
        <errorMessage>Cannot be approved for payment while there is still an exception.  Clear all errors before proceeding</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_be_Finalized_w_bal_nonzero</fullName>
        <active>true</active>
        <description>Cannot finalize the invoice if amount does not equal to the lines roll up amount which means balance due is not zero.</description>
        <errorConditionFormula>Finalized__c = true &amp;&amp;
Invoice_Amount__c !=  Total_Extended_Price__c</errorConditionFormula>
        <errorDisplayField>Finalized__c</errorDisplayField>
        <errorMessage>Invoice can not be finalized, if the lines total does not equal the invoice amount!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Memo_Amount</fullName>
        <active>false</active>
        <description>Credit memo amount can&apos;t be null if the invoice type is Credit Memo</description>
        <errorConditionFormula>IF( ISPICKVAL( Invoice_Type__c , &apos;Credit Memo&apos;) , if(ISBLANK( Credit_Memo_Amount__c ),true,false), false)</errorConditionFormula>
        <errorDisplayField>Credit_Memo_Amount__c</errorDisplayField>
        <errorMessage>You must provide a Credit Memo Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Memo_cannot_be_a_zero_dollar</fullName>
        <active>true</active>
        <description>A Credit memo or Prepayment cannot exist with a zero dollar Invoice amount.</description>
        <errorConditionFormula>(ISPICKVAL(Invoice_Type__c,&apos;Credit Memo&apos;) || ISPICKVAL(Invoice_Type__c,&apos;Prepayment&apos;))&amp;&amp;   (Invoice_Amount__c == 0.00)</errorConditionFormula>
        <errorMessage>You can&apos;t save a supplier invoice if the Invoice Type field is Credit Memo or Prepayment and the Invoice Amount field is 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_taken_cannot_be_negative</fullName>
        <active>true</active>
        <errorConditionFormula>Discount_Taken__c &lt; 0</errorConditionFormula>
        <errorMessage>Discount taken cannot be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Amount_not_null</fullName>
        <active>true</active>
        <description>Invoice Amount can&apos;t be null if the invoice type is Invoice</description>
        <errorConditionFormula>IF( ISPICKVAL( Invoice_Type__c , &apos;Invoice&apos;) , if(ISBLANK( Invoice_Amount__c ),true,false), false)</errorConditionFormula>
        <errorDisplayField>Invoice_Amount__c</errorDisplayField>
        <errorMessage>You must provide an Invoice Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Type_is_required</fullName>
        <active>true</active>
        <description>Invoice Type of either invoice or credit memo or prepayment is required.</description>
        <errorConditionFormula>IsPickVal(Invoice_Type__c, &quot;&quot;) || 
(Not(IsPickVal (Invoice_Type__c, &quot;Invoice&quot;))&amp;&amp;
Not(IsPickVal (Invoice_Type__c, &quot;Credit Memo&quot;)) &amp;&amp;
Not(IsPickVal (Invoice_Type__c, &quot;Prepayment&quot;)))</errorConditionFormula>
        <errorDisplayField>Invoice_Type__c</errorDisplayField>
        <errorMessage>Invoice Type of either invoice or credit memo or prepayment is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_cannot_change_after_payment</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_Paid__c &gt; 0 &amp;&amp;
(ISCHANGED ( Company__c ) ||
ISCHANGED (  Supplier__c  ) ||
ISCHANGED (  Invoice_Amount__c ) ||
ISCHANGED (  Payment_Term__c  ) ||
ISCHANGED (  Date_Invoiced__c  ) ||
ISCHANGED (  Invoice_Type__c  ))</errorConditionFormula>
        <errorMessage>You cannot change this invoice once payments have been made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Balance_based_on_Invoice_Amount</fullName>
        <active>true</active>
        <description>throw an error in case  balance/credit exceeds.</description>
        <errorConditionFormula>if(
AND(
Not(ISPICKVAL( Invoice_Type__c , &apos;Credit Memo&apos;)),ISPICKVAL(  Status__c  , &apos;Approved&apos;) ),  Balance_Due__c &lt; 0 ,
 Credit_Memo_Balance__c &gt; 0)</errorConditionFormula>
        <errorMessage>You can&apos;t enter an Amount to Apply that&apos;s greater than the balance due. Review the selected invoices to pay to find the correct amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Supplier_invoice_number_not_null</fullName>
        <active>true</active>
        <description>Supplier invoice number can&apos;t be null if the invoice type is Invoice</description>
        <errorConditionFormula>IF( ISPICKVAL( Invoice_Type__c , &apos;Invoice&apos;) , if(ISBLANK(Supplier_Invoice__c),true,false), false)</errorConditionFormula>
        <errorDisplayField>Supplier_Invoice__c</errorDisplayField>
        <errorMessage>You must provide a Supplier Invoice Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Void_and_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>Void__c &amp;&amp; !Finalized__c</errorConditionFormula>
        <errorMessage>This Supplier Invoice must be Finalized in order to Void</errorMessage>
    </validationRules>
