    <validationRules>
        <fullName>Bank_Routing_Required_for_ACH</fullName>
        <active>true</active>
        <description>Bank Routing information is required for ACH payment method</description>
        <errorConditionFormula>AND( ISPICKVAL( Payment_Method_G__c , &apos;ACH&apos;),
len( Bank_Routing_Number__c )=0)</errorConditionFormula>
        <errorDisplayField>Bank_Routing_Number__c</errorDisplayField>
        <errorMessage>Bank Routing Number is required for ACH Transfers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_TIN_Length</fullName>
        <active>false</active>
        <description>Max TIN Length</description>
        <errorConditionFormula>Not(IsBlank(Tax_ID__c)) &amp;&amp; LEN(Tax_ID__c) &gt; 11</errorConditionFormula>
        <errorDisplayField>Tax_ID__c</errorDisplayField>
        <errorMessage>Tax identification number exceeds the valid length.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Bank_Account_for_ACH</fullName>
        <active>true</active>
        <description>For ACH transfers, we need to make sure we have Bank Account info</description>
        <errorConditionFormula>AND(ISPICKVAL( Payment_Method_G__c , &apos;ACH&apos;), LEN(Bank_Account_Number__c)=0)</errorConditionFormula>
        <errorDisplayField>Bank_Account_Number__c</errorDisplayField>
        <errorMessage>Bank Account Number Required for ACH transfers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>x1099_Box_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>if(X1099_Supplier__c &amp;&amp;  ISPICKVAL(X1099_Box_G__c ,&quot;&quot;)
,true, false)</errorConditionFormula>
        <errorDisplayField>X1099_Box_G__c</errorDisplayField>
        <errorMessage>You must select a 1099 Box value for a 1099 reportable supplier.</errorMessage>
    </validationRules>
