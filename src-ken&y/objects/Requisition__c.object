        <fullName>Past_Transaction_Date</fullName>
        <active>true</active>
        <errorConditionFormula>(
    (ISNEW() || ISCHANGED(Transaction_Date__c)) 
    &amp;&amp;
    Transaction_Date__c &lt; DATEVALUE(NOW())
)</errorConditionFormula>
        <errorDisplayField>Transaction_Date__c</errorDisplayField>
        <errorMessage>Transaction Date cannot be set to a past date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Validation</fullName>
        <active>true</active>
        <description>Currently Requisitions only supports the type: &quot;Purchase Requisition&quot;</description>
        <errorConditionFormula>NOT(ISPICKVAL(Type__c,&apos;Purchase Requisition&apos;))</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
