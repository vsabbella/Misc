    <validationRules>
        <fullName>Amount_is_required_on_Bank_Transaction</fullName>
        <active>true</active>
        <description>Amount is required on Bank Transaction</description>
        <errorConditionFormula>OR(ISBLANK(Amount__c),
   Amount__c = 0)</errorConditionFormula>
        <errorMessage>Amount cannot be blank or zero. Please populate valid amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BT_can_not_be_added</fullName>
        <active>true</active>
        <description>Bank Transaction can&apos;t be added on finalized bank transaction group.</description>
        <errorConditionFormula>AND(Bank_Transaction_Group__r.Finalized__c = true,
ISNEW())</errorConditionFormula>
        <errorMessage>Can&apos;t add more Bank Transaction as it is already finalized.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Account_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Bank_Account__c)</errorConditionFormula>
        <errorMessage>Please populate &apos;Bank Account&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_change_finalized_flag</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Finalized_2__c),
Finalized_2__c,
ISPICKVAL(Bank_Transaction_Group__r.Status__c, &apos;Cancelled&apos;))</errorConditionFormula>
        <errorMessage>Bank Transaction cannot be finalized if it is cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_manually_unreconcile</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Reconciled_As_Of_Date__c) &amp;&amp;  Reconciled_Hidden__c = TRUE</errorConditionFormula>
        <errorMessage>Cannot un-reconcile manually.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_set_reconciled_date_if_unfinalized</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Reconciled_As_Of_Date__c )) &amp;&amp;  Finalized__c = false</errorConditionFormula>
        <errorDisplayField>Reconciled_As_Of_Date__c</errorDisplayField>
        <errorMessage>Cannot set reconcile if bank transaction is not approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_set_reconciled_date_manually</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Reconciled_As_Of_Date__c )) &amp;&amp;   Reconciled_Hidden__c  = false</errorConditionFormula>
        <errorDisplayField>Reconciled_As_Of_Date__c</errorDisplayField>
        <errorMessage>Cannot set reconcile manually</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Currency_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>Currency__c  = null</errorConditionFormula>
        <errorMessage>Please populate &apos;Currency&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Date_is_mandatory</fullName>
        <active>true</active>
        <description>Transaction Date is mandatory</description>
        <errorConditionFormula>OR(ISBLANK(Transaction_Date__c),
   ISNULL(Transaction_Date__c)
)</errorConditionFormula>
        <errorMessage>Please populate &apos;Transaction Date&apos;</errorMessage>
    </validationRules>
