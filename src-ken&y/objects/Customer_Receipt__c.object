    <validationRules>
        <fullName>Amount_Received_and_Bank_Account</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
NOT(ISBLANK(Amount_Received__c )) &amp;&amp; Amount_Received__c &gt; 0 &amp;&amp; AND(ISBLANK(Bank_Account_2__c), ISBLANK(Payment_Method_3__c)),
NOT(ISBLANK(Bank_Account_2__c)) &amp;&amp;  ISBLANK(Amount_Received__c )
) 
&amp;&amp; Finalized__c = false &amp;&amp; NOT(ISCHANGED(Finalized__c))</errorConditionFormula>
        <errorMessage>Need to populate both Amount Received and Bank Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Account_should_match_with_Company</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Bank_Account_2__c != null, 
Bank_Account_2__r.Company__c &lt;&gt; Company__c)</errorConditionFormula>
        <errorMessage>Bank Account should be associated with the Company mentioned on this receipt. Please update it accordingly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_account_have_same_currency</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Bank_Account_2__c )) &amp;&amp;   Currency__r.ISO_Code__c  !=   Bank_Account_2__r.Currency__r.ISO_Code__c</errorConditionFormula>
        <errorMessage>You can&apos;t create a receipt using a different currency from the bank account&apos;s currency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_account_must_have_same_currency</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISBLANK( Bank_Account_2__c )) &amp;&amp;     
TEXT( Transaction_Currency__c ) !=    
TEXT( Bank_Account_2__r.Transaction_Currency__c )</errorConditionFormula>
        <errorMessage>Bank account must have the same transaction currency as the customer receipt</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancelled_not_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) == &apos;Cancelled&apos; &amp;&amp; Finalized__c</errorConditionFormula>
        <errorMessage>You cannot Finalize a Customer Receipt that has been cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finalized_Check</fullName>
        <active>true</active>
        <description>Finalized checkbox should not be checked when there is amount unapplied left on the record.</description>
        <errorConditionFormula>IF( AND( ISPICKVAL(Type__c, &apos;Receipt&apos;), Amount_Unapplied__c != 0,  Finalized__c == TRUE), TRUE, FALSE )</errorConditionFormula>
        <errorMessage>This customer receipt is not ready to be finalized yet. The entire amount should be applied!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Method_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>false</errorConditionFormula>
        <errorMessage>Please enter a payment method</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_method_and_Reference_Numvber</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Bank_Account_2__c )) &amp;&amp; ISBLANK( TEXT(Payment_Method_G__c)) &amp;&amp; ISBLANK( Reference_Number__c )</errorConditionFormula>
        <errorMessage>Please specify payment method and reference number</errorMessage>
    </validationRules>
