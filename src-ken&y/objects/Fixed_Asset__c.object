        <searchResultsAdditionalFields>Asset_Class__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Depreciation_Method__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>In_Service_Date__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Acquisition_Cost_Cannot_Be_Negative</fullName>
        <active>true</active>
        <description>You cannot have a negative acquisition cost.</description>
        <errorConditionFormula>Acquisition_Cost__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Acquisition_Cost__c</errorDisplayField>
        <errorMessage>You cannot have a negative acquisition cost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Depreciation_Conv_with_Prorate</fullName>
        <active>true</active>
        <description>2b) The option can only be selected when the Depreciation Convention is Full Month.</description>
        <errorConditionFormula>ISPICKVAL(Depreciation_Convention__c, &apos;Mid Month&apos;) 
   &amp;&amp; Prorate_First_Period__c = true</errorConditionFormula>
        <errorDisplayField>Prorate_First_Period__c</errorDisplayField>
        <errorMessage>The Prorate  option can only be selected when the Depreciation Convention is Full Month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Depr_Period_is_Monthly_for_Prorate</fullName>
        <active>true</active>
        <description>The Prorate option can only be selected when the Depreciation Period is Monthly</description>
        <errorConditionFormula>Prorate_First_Period__c = true &amp;&amp; 
					   ( ISPICKVAL(Depreciation_Period__c, &apos;Quarterly&apos;) ||
					    ISPICKVAL(Depreciation_Period__c, &apos;Annually&apos;) )</errorConditionFormula>
        <errorMessage>The Prorate option can only be selected when the Depreciation Period is Monthly</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Depreciable_Basis_cannot_be_negative</fullName>
        <active>true</active>
        <description>You cannot have a negative depreciable basis on a Fixed Asset.</description>
        <errorConditionFormula>Depreciable_Basis__c &lt; 0</errorConditionFormula>
        <errorMessage>You cannot have a negative depreciable basis on a Fixed Asset.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Prorate_after_asset_depreciation</fullName>
        <active>true</active>
        <description>The user should be blocked from selecting prorate checkbox if the asset has already depreciated</description>
        <errorConditionFormula>AND(
 ISCHANGED(Prorate_First_Period__c),
 NOT(ISBLANK(Date_Last_Depreciated__c)),
 Prorate_First_Period__c = true ,
 Total_Depreciated_Value__c!=0)</errorConditionFormula>
        <errorDisplayField>Prorate_First_Period__c</errorDisplayField>
        <errorMessage>Pro-rate can only be done before the first depreciation. please Roll Back depreciation and try again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Residual_Value_can_not_be_null</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL(Residual_Value__c)</errorConditionFormula>
        <errorMessage>Please enter a value for Residual Value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>The_Fixed_asset_company_must_match_the_f</fullName>
        <active>true</active>
        <description>The Fixed asset company must match the fixed asset class company</description>
        <errorConditionFormula>IF(ISNULL(Asset_Class__c) = FALSE &amp;&amp; Asset_Class__r.Company__c != Company__c, TRUE, FALSE)</errorConditionFormula>
        <errorMessage>The Fixed asset company must match the fixed asset class company</errorMessage>
    </validationRules>
