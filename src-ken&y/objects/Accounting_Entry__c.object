Period__r.Open__c,

OR( 

AND( 
OR( 
NOT(ISBLANK(Customer_Invoice__c)), 
NOT(ISBLANK(Bank_Transaction_Group__c)),  
NOT(ISBLANK(Customer_Receipt__c)) 
), 
ISNULL(Period__r.AR_Subledger_Closed__c), 
NOW() &gt;= IF(ISNULL(Period__r.AR_Valid_After__c), Period__r.AR_Subledger_Cutoff__c, Period__r.AR_Valid_After__c) 
), 

AND( 
OR( 
NOT(ISBLANK(Supplier_Invoice__c)), 
NOT(ISBLANK(Supplier_Payment__c))
),
ISNULL(Period__r.AP_Subledger_Closed__c), 
NOW() &gt;= IF(ISNULL(Period__r.AP_Valid_After__c), Period__r.AP_Subledger_Cutoff__c, Period__r.AP_Valid_After__c) 
), 

AND( 
OR( 
NOT(ISBLANK(Work_Order__c)), 
NOT(ISBLANK(Inventory_Change__c)) 
), 
ISNULL(Period__r.Inventory_Subledger_Closed__c), 
NOW() &gt;= IF(ISNULL(Period__r.Inventory_Valid_After__c), Period__r.Inventory_Subledger_Cutoff__c, Period__r.Inventory_Valid_After__c) 
), 

AND( 
OR( 
NOT(ISBLANK(Fixed_Asset_Adjustment__c)), 
NOT(ISBLANK(Fixed_Asset_Depreciation__c)) 
), 
ISNULL(Period__r.FA_Subledger_Closed__c), 
NOW() &gt;= IF(ISNULL(Period__r.FA_Valid_After__c), Period__r.FA_Subledger_Cutoff__c, Period__r.FA_Valid_After__c) 
), 

AND( 
NOT(ISBLANK(Shipment__c)), 
ISNULL(Period__r.Shipment_Subledger_Closed__c), 
NOW() &gt;= IF(ISNULL(Period__r.Shipment_Valid_After__c), Period__r.Shipment_Subledger_Cutoff__c, Period__r.Shipment_Valid_After__c) 
), 

AND( 
NOT(ISBLANK(Receiving_Document__c)), 
ISNULL(Period__r.Receiving_Subledger_Closed__c), 
NOW() &gt;= IF(ISNULL(Period__r.Receiving_Valid_After__c), Period__r.Receiving_Subledger_Cutoff__c, Period__r.Receiving_Valid_After__c) 
), 

AND( 
NOT(ISBLANK(Journal_Entry__c)), 
ISNULL(Period__r.GL_Close__c), 
NOW() &gt;= IF(ISNULL(Period__r.JE_Valid_After__c), Period__r.JE_Cutoff__c, Period__r.JE_Valid_After__c) 
) 

, 

AND( 
OR( 
NOT(ISBLANK(Invoice__c)), 
