        <formula>OR(

AND( 
  OR(
   NOT(ISBLANK(Customer_Invoice__c)),
   NOT(ISBLANK(Customer_Receipt__c))
    ), 
  NOT(ISBLANK(Period__r.AR_Subledger_Closed__c))
 ),

AND( 
  OR(
   NOT(ISBLANK(Supplier_Invoice__c)),
   NOT(ISBLANK(Supplier_Payment__c))
    ), 
  NOT(ISBLANK(Period__r.AP_Subledger_Closed__c))
 ),

AND( 
  OR(
   NOT(ISBLANK(Invoice__c)),
   NOT(ISBLANK(Payment__c))
    ), 
  NOT(ISBLANK(Period__r.InterCompany_Subledger_Closed__c))
 ),

AND( 
   NOT(ISBLANK(Receiving_Document__c)), 
   NOT(ISBLANK(Period__r.Receiving_Subledger_Closed__c))
 ),

AND( 
   NOT(ISBLANK(Shipment__c)), 
   NOT(ISBLANK(Period__r.Shipment_Subledger_Closed__c))
 ),

AND( 
  OR(
   NOT(ISBLANK(Work_Order__c)),
   NOT(ISBLANK(Inventory_Change__c))
    ), 
  NOT(ISBLANK(Period__r.Inventory_Subledger_Closed__c))
 ),


AND( 
  OR(
   NOT(ISBLANK(Fixed_Asset_Depreciation__c)),
   NOT(ISBLANK(Fixed_Asset_Adjustment__c))
    ), 
  NOT(ISBLANK(Period__r.FA_Subledger_Closed__c))
 ),

AND( 
   NOT(ISBLANK(Journal_Entry__c)), 
   NOT(ISBLANK(Period__r.GL_Close__c))
 )

,

AND( 
   OR(
   NOT(ISBLANK(Invoice__c)),
   NOT(ISBLANK(Payment__c))
    ), 
   NOT(ISBLANK(Period__r.InterCompany_Subledger_Closed__c))
 )
)</formula>
