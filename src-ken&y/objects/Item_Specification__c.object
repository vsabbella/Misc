    <validationRules>
        <fullName>Company_must_not_change</fullName>
        <active>true</active>
        <description>after creating a Item Specification for a given company, do not allow the Item Specification to change to another company</description>
        <errorConditionFormula>ISCHANGED(Company__c)</errorConditionFormula>
        <errorMessage>Once created for a Company, the Item Specification must not change Company field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_fields</fullName>
        <active>true</active>
        <description>Required fields</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Type__c, &quot;Template&quot;)), OR(ISBLANK(Item__c), ISNULL(Item__c), ISBLANK(Company__c), ISNULL(Company__c), ISBLANK(Start_Date2__c), ISNULL(Start_Date2__c)))</errorConditionFormula>
        <errorMessage>Item, company and start date are required for non-template item specifications</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Revision_required_for_item_specification</fullName>
        <active>true</active>
        <description>Revision is  required for  item specifications.</description>
        <errorConditionFormula>OR(ISBLANK( Revision__c ),ISNULL(Revision__c))</errorConditionFormula>
        <errorMessage>Revision is  required for  item specifications.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Uncheck_Costing_and_Production</fullName>
        <active>true</active>
        <description>Uncheck Costing and Production</description>
        <errorConditionFormula>AND(OR(Configurable__c,ISPICKVAL(Type__c,&quot;Template&quot;)),OR(Costing__c,Production__c))</errorConditionFormula>
        <errorMessage>Configurable or template item specifications cannot have production or costing checked</errorMessage>
