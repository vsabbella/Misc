    <fields>
        <fullName>Description__c</fullName>
        <externalId>false</externalId>
        <formula>IF(!IsBlank(Company__r.Name), &apos;For &apos;+ Company__r.Name+&apos;,&apos;,NULL) + IF(!IsBlank(Bank_Account__r.Name), &apos;For &apos;+ Bank_Account__r.Name+&apos;,&apos;,NULL) + IF(!IsBlank(Documents__c), &apos; For &apos;,NULL) + IF(INCLUDES( Documents__c, &quot;Customer Receipts&quot;), &quot; Customer Receipts,&quot;, NULL) + IF(INCLUDES( Documents__c , &quot;Supplier Payments&quot;), &quot;Supplier Payments,&quot;, NULL)  + IF(INCLUDES( Documents__c, &quot;Payments&quot;), &quot;Payments&quot;, NULL) + IF(INCLUDES( Documents__c, &quot;Journal Entry Lines&quot;), &quot;Journal Entry Lines&quot;, NULL) + IF(Amount__c &amp;&amp; IsBlank(Amount_Threshold__c),&apos;, Match on Amount&apos; , NULL) + IF(!IsBlank(Amount_Threshold__c) &amp;&amp; Amount__c, &apos;, Match on Amount(+/-  &apos;+ Text(Amount_Threshold__c)+&apos;$)&apos;,NULL) + IF(Transaction_Date__c &amp;&amp; IsBlank(Date_Threshold__c),&apos;, Match on Transaction Date&apos; , NULL ) + IF(!IsBlank(Date_Threshold__c) &amp;&amp; Transaction_Date__c, &apos;, Match on Transaction Date(+/-  &apos;+ Text(Date_Threshold__c) +&apos; days)&apos;,NULL) +   IF(Reference__c,&apos;, Match on Reference Number&apos;,NULL) + IF(Transacted_With__c,&apos;, Match on Transacted With&apos;,NULL) + IF(Keep_In_Match_Pool__c,&apos;, Keep in Match Pool&apos;,&apos;, Do not Keep in Match Pool&apos;) + IF(Auto_Approve__c,&apos;, Auto Approve&apos;,NULL)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Description</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
