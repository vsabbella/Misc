    <validationRules>
        <fullName>Amount_Threshold_Cannot_be_Negative</fullName>
        <active>true</active>
        <description>Amount Threshold value cannot be a negative amount.</description>
        <errorConditionFormula>Amount_Threshold__c &lt; 0.00</errorConditionFormula>
        <errorMessage>Amount Threshold cannot be a negative value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Atleast_one_matching_criteria_Selection</fullName>
        <active>true</active>
        <description>To set a Bank Transaction Matching Rule, atleast one matching criteria should be selected.</description>
        <errorConditionFormula>AND( NOT(Amount__c),AND(NOT(ISBLANK(Amount_Threshold__c)), Amount_Threshold__c == 0.00),NOT(Transaction_Date__c),AND(NOT(ISBLANK(Date_Threshold__c)), Date_Threshold__c == 0),NOT(Auto_Approve__c),NOT(Keep_In_Match_Pool__c),NOT(Auto_Approve__c), NOT(Transacted_With__c),NOT(Reference__c))</errorConditionFormula>
        <errorMessage>Atleast one Matching Criteria should be added to the rule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Account_Association_with_Company</fullName>
        <active>true</active>
        <description>Bank Account should be associated with the company selected on the Bank Transaction Matching Rules.</description>
        <errorConditionFormula>OR(ISNULL(Bank_Account__c), AND(NOT(ISBLANK(Bank_Account__c)),(Company__c !=  Bank_Account__r.Company__c)))</errorConditionFormula>
        <errorMessage>Bank Account is not associated with the company.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_CheckBoth_AutoApprove_KeepMatchPL</fullName>
        <active>true</active>
        <description>Both Auto Approve and Keep in Match Pool can be set both for the Matching Rule.</description>
        <errorConditionFormula>AND(Auto_Approve__c,Keep_In_Match_Pool__c)</errorConditionFormula>
        <errorMessage>Cannot set both Keep in Match Pool and Auto Approve.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Threshold_Cannot_be_Negative</fullName>
        <active>true</active>
        <description>Date Threshold value cannot be negative days.</description>
        <errorConditionFormula>Date_Threshold__c &lt; 0</errorConditionFormula>
        <errorMessage>Date Threshold cannot be a negative value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Documents_are_Required</fullName>
        <active>true</active>
        <description>Atleast one document is required.</description>
        <errorConditionFormula>ISNULL(Documents__c)</errorConditionFormula>
        <errorMessage>Atleast one Document is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Select_Amount_Matching_Criteria</fullName>
        <active>true</active>
        <description>Amount threshold can be set only if the amount matching criteria is checked.</description>
        <errorConditionFormula>AND(NOT(Amount__c),AND(NOT(ISBLANK(Amount_Threshold__c)), Amount_Threshold__c != 0.00))</errorConditionFormula>
        <errorMessage>Amount threshold can only be set if the Amount matching criteria is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Select_Transaction_Date_Criteria</fullName>
        <active>true</active>
        <description>Days threshold can be set only if the transaction date matching criteria is checked.</description>
        <errorConditionFormula>AND(NOT(Transaction_Date__c),AND(NOT(ISBLANK(Date_Threshold__c)), Date_Threshold__c != 0))</errorConditionFormula>
        <errorMessage>Days threshold can only be set if the Transaction Date matching criteria is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_is_Required</fullName>
        <active>true</active>
        <description>Start Date is required.</description>
        <errorConditionFormula>ISNULL(Start_Date__c )</errorConditionFormula>
        <errorMessage>Start Date is required.</errorMessage>
    </validationRules>
