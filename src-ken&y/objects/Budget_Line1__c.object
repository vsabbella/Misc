        <active>true</active>
        <errorConditionFormula>(Period__r.Period_Start__c &lt;  Budget__r.Budget_From__r.Period_Start__c) || (Period__r.Period_Start__c &gt;= Budget__r.Budget_To__r.Period_End__c)</errorConditionFormula>
        <errorDisplayField>Period__c</errorDisplayField>
        <errorMessage>Period Start Date cannot be less than the Budget From Period Start or greater than or equal to the Budget To Period End</errorMessage>
