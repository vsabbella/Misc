    <validationRules>
        <fullName>Cancelled_not_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) == &apos;Cancelled&apos; &amp;&amp; Finalized__c</errorConditionFormula>
        <errorDisplayField>Finalized__c</errorDisplayField>
        <errorMessage>You cannot Finalize a Customer Invoice that has been cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Unfinalize_paid_invoices</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Finalized__c ) &amp;&amp; Finalized__c == false &amp;&amp;  Balance__c !=  Total_Amount__c &amp;&amp; NOT( ISPICKVAL( Status__c , &quot;Cancelled&quot;) ) &amp;&amp; NOT( ISPICKVAL( Status__c , &quot;Void&quot;) )</errorConditionFormula>
        <errorDisplayField>Finalized__c</errorDisplayField>
        <errorMessage>Cannot unfinalize an invoice if it is partially/fully paid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Receipt_Does_Not_Match</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK(Customer_Receipt_Line__c) ) ,Customer_Receipt_Line__r.Customer_Receipt__c !=  Customer_Receipt__c)</errorConditionFormula>
        <errorDisplayField>Customer_Receipt_Line__c</errorDisplayField>
        <errorMessage>The Receipt Line must be a child of Customer Invoice -&gt; Customer Receipt</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Freeze_Due_Date_validation</fullName>
        <active>true</active>
        <description>If the Freeze Due Date is set, the Due date on the customer invoice cannot be changed</description>
        <errorConditionFormula>(Freeze_Due_Date__c &amp;&amp; ISCHANGED(Due_Date__c))</errorConditionFormula>
        <errorMessage>You cannot change the Due Date as it is frozen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_invoice_cant_link_to_sales_order</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Invoice_Type__c , &quot;Manual&quot;) &amp;&amp; NOT(ISBLANK( Sales_Order__c ))</errorConditionFormula>
        <errorDisplayField>Sales_Order__c</errorDisplayField>
        <errorMessage>Manual invoice cannot be linked to a sales order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Terms</fullName>
        <active>true</active>
        <description>Checks to make sure that payment terns are filled out when status is &quot;invoiced&quot;.</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;Invoiced&quot;)  &amp;&amp;  ISBLANK(Payment_Term__c)</errorConditionFormula>
        <errorMessage>Enter Payment Terms on Sales Order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Balance_based_on_Total_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(Total_Amount__c&gt;0,Balance__c&lt;0),
 AND(Total_Amount__c&lt;0,Balance__c&gt;0))</errorConditionFormula>
        <errorMessage>You cannot apply more than the invoice balance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ship_Date_for_Shipped_status</fullName>
        <active>true</active>
        <description>Ship date should be entered for shipments having status as shipped</description>
        <errorConditionFormula>IF(ISPICKVAL( Status__c , &apos;shipped&apos;) &amp;&amp;  ISBLANK(Ship_Date__c)  , true, false)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please enter a ship date</errorMessage>
    </validationRules>
