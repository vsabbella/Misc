    <fields>
        <fullName>Type__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Specifies whether the component is to be kitted, backflushed, or is a by-product of previously manufactured finished goods. To account for a by-product component being used in this assembly,  specify the Quantity Per Assembly as a negative amount.</inlineHelpText>
        <label>Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Kit</fullName>
                    <default>false</default>
                </value>
                <value>
                    <fullName>Backflush</fullName>
                    <default>false</default>
                </value>
                <value>
                    <fullName>By-product</fullName>
                    <default>false</default>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Yield__c</fullName>
        <defaultValue>1</defaultValue>
        <description>Yield precentage applied to component on this bill.  Enter as percentage (default is 100, meaning 100% yield).</description>
        <externalId>false</externalId>
        <inlineHelpText>Yield that applies to this component on this BOM. The default is 100% - you do not need to enter the % symbol.</inlineHelpText>
        <label>Yield</label>
        <precision>5</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
