    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Company_must_match</fullName>
        <active>true</active>
        <description>Company must natch header&apos;s company</description>
        <errorConditionFormula>Assembly_Item_Specification__r.Company__c !=  Company__c</errorConditionFormula>
        <errorMessage>Component Company must match Item Specification Company.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_Sequence_cannot_be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>Component_Sequence__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Component_Sequence__c</errorDisplayField>
        <errorMessage>Component Sequence cannot be Negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_Sequence_should_be_blank</fullName>
        <active>true</active>
        <description>Component sequence should be blank if Component Type selected is By Product.</description>
        <errorConditionFormula>ISPICKVAL(Type__c,&apos;By-product&apos;)   &amp;&amp;  ! ISBLANK(Component_Sequence__c)</errorConditionFormula>
        <errorDisplayField>Component_Sequence__c</errorDisplayField>
        <errorMessage>Component sequence should be blank if Component Type selected is By Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Component_cannot_equal_assembly</fullName>
        <active>true</active>
        <description>The component item and assembly item cannot be the same</description>
        <errorConditionFormula>Assembly_Item_Specification__r.Item__c  =  Component_Item__c</errorConditionFormula>
        <errorDisplayField>Component_Item__c</errorDisplayField>
        <errorMessage>The component item and assembly item cannot be the same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_value_check</fullName>
        <active>true</active>
        <description>Check if the quantity per assembly is valid</description>
        <errorConditionFormula>NOT(Quantity_per_Assembly__c &gt;0)</errorConditionFormula>
        <errorDisplayField>Quantity_per_Assembly__c</errorDisplayField>
        <errorMessage>Quantity per assembly should greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Yield_Value_Check</fullName>
        <active>true</active>
        <description>Yield must be greater than 0% and less or equal to 100%</description>
        <errorConditionFormula>OR(Yield__c &lt;=0,Yield__c &gt;1)</errorConditionFormula>
        <errorDisplayField>Yield__c</errorDisplayField>
        <errorMessage>Yield must be greater than 0% and less or equal to 100%</errorMessage>
    </validationRules>
</CustomObject>
