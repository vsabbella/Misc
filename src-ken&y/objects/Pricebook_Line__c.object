    <validationRules>
        <fullName>Block_New_Lines_in_past_dates</fullName>
        <active>true</active>
        <description>Blocks creation of new pricebook lines in past dates</description>
        <errorConditionFormula>AND(
ISNEW(), 
(OR(Start_Date__c &lt; TODAY(), End_Date__c &lt; TODAY())),
Data_Load__c = false
)</errorConditionFormula>
        <errorMessage>Cannot create lines with historical dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_of_Product_or_Product_Group_Reqd</fullName>
        <active>true</active>
        <description>User can&apos;t enter both Product and Product group, only one should be present on the Pricebook line record</description>
        <errorConditionFormula>OR (
	AND (
		 ISBLANK( Product_Group__c ),
		 ISBLANK(  Product__c  ) 
	)	
,
	AND (
		NOT( ISBLANK( Product_Group__c ) ),
		NOT( ISBLANK(  Product__c  ) )
	)
)</errorConditionFormula>
        <errorMessage>Either a Product or Product Group is required.</errorMessage>
    </validationRules>
