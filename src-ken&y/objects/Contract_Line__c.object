    <validationRules>
        <fullName>Agreement_Qty_less_than_Purchased_Qty</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(Contract_Quantity__c) &amp;&amp; Contract_Quantity__c &lt;  Purchased_Quantity__c</errorConditionFormula>
        <errorDisplayField>Contract_Quantity__c</errorDisplayField>
        <errorMessage>Cannot reduce the Purchase Agreement Quantity below the Purchased Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_invoice_matching</fullName>
        <active>true</active>
        <description>Validation rule to prevent changing the invoice matching on an approved agreement line</description>
        <errorConditionFormula>AND( ISCHANGED( Invoice_Matching__c ) , 
Approved__c )</errorConditionFormula>
        <errorMessage>Cannot update invoice matching field on an approved purchase agreement line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Line_End_date_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND
     ((ISNEW() || ISCHANGED(  Contract_Start__c  ) || 
      ISCHANGED( Contract_End__c ) ),  
      Contract_End__c  &lt;  Contract_Start__c )</errorConditionFormula>
        <errorMessage>Contract line End date should not be less than Contract line Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ForceOverrideCPCCode</fullName>
        <active>true</active>
        <description>If the ASL Override is enabled, force the user to enter a CPC code</description>
        <errorConditionFormula>ASL_Override__c == true &amp;&amp;  OR( ISBLANK( CPC_Code__c ) ,ISNULL( CPC_Code__c ) )</errorConditionFormula>
        <errorDisplayField>CPC_Code__c</errorDisplayField>
        <errorMessage>CPC code is required if ASL Override is enabled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_is_required</fullName>
        <active>true</active>
        <description>Item is required</description>
        <errorConditionFormula>Item__c == &apos;&apos;</errorConditionFormula>
        <errorMessage>Item is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MOQ_cannot_be_negative</fullName>
        <active>true</active>
        <description>Minimum Order Quantity cannot be negative</description>
        <errorConditionFormula>Minimum_Order_Quantity__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Minimum_Order_Quantity__c</errorDisplayField>
        <errorMessage>Minimum Order Quantity cannot be a negative number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negotiated_Price_cannot_be_negative</fullName>
        <active>true</active>
        <description>Negotiated Price cannot be negative</description>
        <errorConditionFormula>Negotiated_Price__c  &lt; 0</errorConditionFormula>
        <errorMessage>Negotiated Price cannot be negative</errorMessage>
    </validationRules>
