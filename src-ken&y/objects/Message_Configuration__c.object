<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting is used to manage which chatter groups are mentioned for batch messaging.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>ExtId__c</fullName>
        <caseSensitive>true</caseSensitive>
        <externalId>true</externalId>
        <label>External Id</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Group__c</fullName>
        <description>The group that is being mentioned in the message.</description>
        <externalId>false</externalId>
        <inlineHelpText>The group that is being mentioned in the message.</inlineHelpText>
        <label>Group</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Process_Name__c</fullName>
        <description>The name of the process delivering messages.</description>
        <externalId>false</externalId>
        <inlineHelpText>The name of the process delivering messages.</inlineHelpText>
        <label>Process Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Message Configuration</label>
    <visibility>Public</visibility>
</CustomObject>
