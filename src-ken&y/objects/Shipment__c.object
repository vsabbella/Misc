    <fields>
        <fullName>Weight_Out_Of_Range__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
Customer__r.Aggregate_Shipments__c,  
IF(Means_of_Transport__r.Maximum_Weight__c &gt; 0, 
   Total_Weight_Planned__c  &gt;  Means_of_Transport__r.Maximum_Weight__c, 
   false
),
false)</formula>
        <label>Weight Out Of Range</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
