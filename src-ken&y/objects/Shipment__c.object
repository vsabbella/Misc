    <fields>
        <fullName>Maximum_Dimensional_Percent__c</fullName>
        <externalId>false</externalId>
        <formula>IF (ISBLANK (Means_of_Transport__c), null, 
	MAX ( 
		IF ( OR(ISBLANK(Means_of_Transport__r.Maximum_Cube__c), ISNULL(Means_of_Transport__r.Maximum_Cube__c), Means_of_Transport__r.Maximum_Cube__c=0), 0, Total_Product_Volume_Planned__c / Means_of_Transport__r.Maximum_Cube__c), 
		IF ( OR(ISBLANK(Means_of_Transport__r.Maximum_Weight__c), ISNULL(Means_of_Transport__r.Maximum_Weight__c), Means_of_Transport__r.Maximum_Weight__c=0), 0, Total_Weight_Planned__c / Means_of_Transport__r.Maximum_Weight__c), 
		IF ( OR(ISBLANK(Means_of_Transport__r.Pallet_Positions_double_stacked__c), ISNULL(Means_of_Transport__r.Pallet_Positions_double_stacked__c), Means_of_Transport__r.Pallet_Positions_double_stacked__c=0), 0, CEILING(Number_of_Pallets_Planned__c) / (Means_of_Transport__r.Pallet_Positions_double_stacked__c)) 
	) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Maximum Dimensional %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
