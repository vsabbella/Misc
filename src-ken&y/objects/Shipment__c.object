                </value>
                <value>
                    <fullName>EUR</fullName>
                    <default>false</default>
                </value>
                <value>
                    <fullName>CAD</fullName>
                    <default>false</default>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
