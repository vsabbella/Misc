    <validationRules>
        <fullName>Cancelled_not_Finalized</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) == &apos;Cancelled&apos; &amp;&amp; Finalized__c</errorConditionFormula>
        <errorMessage>You cannot Finalize a Shipment that has been cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Type_Transaction_Type</fullName>
        <active>true</active>
        <description>Shipment type and transaction type cannot be changed</description>
        <errorConditionFormula>(NOT(ISNEW())
&amp;&amp;
ISCHANGED(Type__c)
&amp;&amp;
NOT(ISBLANK(PRIORVALUE(Type__c)))
||
NOT(ISNEW())
&amp;&amp;
ISCHANGED(Transaction_Type__c)
&amp;&amp;
NOT(ISBLANK(PRIORVALUE(Transaction_Type__c)))
)</errorConditionFormula>
        <errorMessage>Shipment type and  transaction type cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Customer_Field</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Customer__c )</errorConditionFormula>
        <errorMessage>Customer cannot be changed for a shipment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Shipment_Ship_To_Address_fields</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Ship_to_Street__c ) || ISCHANGED( Ship_to_City__c ) || ISCHANGED( Ship_to_State__c ) || ISCHANGED( Ship_to_Country__c ) || ISCHANGED( Ship_to_Zip_Code__c )</errorConditionFormula>
        <errorMessage>Ship-To Address fields cannot be modified</errorMessage>
    </validationRules>
