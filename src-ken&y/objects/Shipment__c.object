    <fields>
        <fullName>Total_Number_Of_Lines__c</fullName>
        <description>Total number of lines.</description>
        <externalId>false</externalId>
        <inlineHelpText>Total number of lines.</inlineHelpText>
        <label>Total Number Of Lines</label>
        <summaryForeignKey>Shipment_Line__c.Shipment__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Total_Pallet_Weight_Planned__c</fullName>
        <description>Weight of the Planned Pallets</description>
        <externalId>false</externalId>
        <formula>Customer__r.Pallet_Product__r.Item__r.Weight__c  *  CEILING( Number_of_Pallets_Planned__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Weight of all planned pallets.</inlineHelpText>
        <label>Total Pallet Weight Planned</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Planned_Weight__c</fullName>
        <description>Roll up of planned total weights for each Shipment line.</description>
        <externalId>false</externalId>
        <inlineHelpText>The sum of the  weight from all shipment lines. Planned weight is pulled from the Sales Order. The total shipment weight can be calculated when the pallet requirements are received.</inlineHelpText>
        <label>Total Planned Weight</label>
        <summarizedField>Shipment_Line__c.Planned_Weight__c</summarizedField>
        <summaryForeignKey>Shipment_Line__c.Shipment__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Total_Product_Volume_Planned__c</fullName>
        <externalId>false</externalId>
        <label>Total Product Volume Planned</label>
        <summarizedField>Shipment_Line__c.Planned_Product_Volume__c</summarizedField>
        <summaryForeignKey>Shipment_Line__c.Shipment__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Total_Product_Weight__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The weight of the product without pallets. After the pallet requirements have been received,  the total shipment weight can be calculated.</inlineHelpText>
        <label>Total Product Weight</label>
        <summarizedField>Shipment_Line__c.Product_Weight__c</summarizedField>
        <summaryForeignKey>Shipment_Line__c.Shipment__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Total_Shipment_Weight__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The weight of the shipment, including pallets.</inlineHelpText>
        <label>Total Shipment Weight</label>
        <summarizedField>Shipment_Line__c.Total_Weight__c</summarizedField>
        <summaryForeignKey>Shipment_Line__c.Shipment__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Total_Weight_Planned__c</fullName>
        <description>Weight of the Shipment ( including Pallets )</description>
        <externalId>false</externalId>
        <formula>Total_Pallet_Weight_Planned__c  +  Total_Product_Weight__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The total weight of the shipment including pallets. The weight calculation uses the estimated number of pallets.</inlineHelpText>
        <label>Total Weight Planned</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
