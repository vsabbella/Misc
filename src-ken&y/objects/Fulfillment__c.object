    <validationRules>
        <fullName>Fulfillment_Quantity_cannot_be_Zero</fullName>
        <active>true</active>
        <description>Quantity cannot be Zero on the fulfillment.</description>
        <errorConditionFormula>TEXT(Sales_Order_Line__r.Status__c ) != &apos;Closed&apos; &amp;&amp; Quantity__c = 0</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>You can’t fulfill an order with zero quantity. Enter an order quantity, then try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_enter_reason_code_if_on_hold</fullName>
        <active>true</active>
        <errorConditionFormula>Invoice_Hold__c == true &amp;&amp; ISBLANK( Reason_Code__c )</errorConditionFormula>
        <errorMessage>You must enter a reason code if setting fulfillment on hold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_code_must_of_invoice_hold_type</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Reason_Code__c ))  &amp;&amp;  NOT(ISPICKVAL( Reason_Code__r.Type__c , &quot;Invoice Hold&quot;) )</errorConditionFormula>
        <errorMessage>Reason code must be of invoice hold type.</errorMessage>
    </validationRules>
