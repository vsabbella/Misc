    <validationRules>
        <fullName>Closed_Check</fullName>
        <active>true</active>
        <description>If status is closed you cannot edit any fields</description>
        <errorConditionFormula>ISPICKVAL( PRIORVALUE(Status__c), &apos;Closed&apos;)
            &amp;&amp; NOT(ISCHANGED(Status__c))</errorConditionFormula>
        <errorMessage>Work Order is Not Open. No Changes are allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Company_must_not_change</fullName>
        <active>true</active>
        <description>after creating a WO for a given company, do not allow the WO to change to another company</description>
        <errorConditionFormula>ISCHANGED(Company__c)</errorConditionFormula>
        <errorMessage>Once created for a Company, the WO must not change Company field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Currency_must_match_company_currency</fullName>
        <active>true</active>
        <description>Currency must match company currency</description>
        <errorConditionFormula>Currency__c != Company__r.Default_Transaction_Currency__c</errorConditionFormula>
        <errorDisplayField>Currency__c</errorDisplayField>
        <errorMessage>Currency must match company&apos;s default transaction currency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_Required_For_Repair_WO</fullName>
        <active>true</active>
        <description>Item or Item specification is required for repair work orders</description>
        <errorConditionFormula>AND(ISPICKVAL(Type__c,&quot;Repair&quot;),Assembly_Item_Specification__c = null, Item__c = null)</errorConditionFormula>
        <errorDisplayField>Item__c</errorDisplayField>
        <errorMessage>Item or Item specification is required for repair work orders</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_Spec_Required_For_Regular_WO</fullName>
        <active>true</active>
        <description>Assembly Item Specification is required for regular work orders</description>
        <errorConditionFormula>AND(ISPICKVAL(Type__c,&quot;Regular&quot;),Assembly_Item_Specification__c = null)</errorConditionFormula>
        <errorDisplayField>Assembly_Item_Specification__c</errorDisplayField>
        <errorMessage>Assembly Item Specification is required for regular work orders</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Order_Quantity__c )  &amp;&amp;  Order_Quantity__c &lt; Quantity_Complete__c + Quantity_Scrapped__c</errorConditionFormula>
        <errorDisplayField>Order_Quantity__c</errorDisplayField>
        <errorMessage>Order Quantity cannot be less than Quantity Completed plus Quantity Scrapped</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Quantity_cannot_be_zero_or_less</fullName>
        <active>false</active>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>Order_Quantity__c</errorDisplayField>
        <errorMessage>Order Quantity cannot be zero or negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_vs_Item_Spec_Effective_Date</fullName>
        <active>true</active>
        <description>The Start Date must be on or after the selected Assembly Item Specification Effective Date</description>
        <errorConditionFormula>Assembly_Item_Specification__r.Start_Date2__c &gt; Start_Date2__c</errorConditionFormula>
        <errorDisplayField>Start_Date2__c</errorDisplayField>
        <errorMessage>The Start Date must be after the selected Assembly Item Specification Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Earliest_Date_Check2</fullName>
        <active>true</active>
        <description>Start date cannot be after earliset completion date</description>
        <errorConditionFormula>Start_Date2__c &gt;  Earliest_Completion_Date2__c</errorConditionFormula>
        <errorDisplayField>Start_Date2__c</errorDisplayField>
        <errorMessage>Start date cannot be after earliset completion date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Need_Date_Check</fullName>
        <active>true</active>
        <description>Start date cannot be after need date</description>
        <errorConditionFormula>Start_Date2__c &gt; Need_Date2__c</errorConditionFormula>
        <errorDisplayField>Start_Date2__c</errorDisplayField>
        <errorMessage>Start date cannot be after need date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubContract_WO_item_spec_validation</fullName>
        <active>false</active>
        <description>If the Work order is of type SubContract then the Item Specification should not have By Product components.</description>
        <errorConditionFormula>ISPICKVAL( Type__c , &apos;SubContract&apos;)  &amp;&amp; Assembly_Item_Specification__r.By_Product_Components__c  &lt;&gt; 0</errorConditionFormula>
        <errorMessage>The assembly item specification should have no by-product components.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subcontract_work_order_block_edit</fullName>
        <active>true</active>
        <description>Blocks edit to critical fields if it is a sub contracted work order.</description>
        <errorConditionFormula>IF(OR( ISCHANGED( Assembly_Item_Specification__c ) , ISCHANGED( Company__c ) , ISCHANGED( Facility__c ) , ISCHANGED( Order_Quantity__c ) , ISCHANGED( Type__c )  , ISCHANGED(  Need_Date2__c  )) &amp;&amp; NOT(ISCHANGED( Last_Sync_Date__c )) &amp;&amp;  (TEXT( PRIORVALUE(Type__c)) == &apos;SubContract&apos; ), true, false)</errorConditionFormula>
        <errorMessage>You cannot make this change as the work order is sub contracted!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Type__c )  &amp;&amp;  NOT( ISPICKVAL(Status__c , &apos;Draft&apos;))</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>You can only change Type if Work Order is Status Draft</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>makeToOrder_work_order_block_edit</fullName>
        <active>true</active>
        <description>Blocks edit to critical fields if it is a sub contracted work order.</description>
        <errorConditionFormula>IF(OR( ISCHANGED( Assembly_Item_Specification__c ) , ISCHANGED( Company__c ) , ISCHANGED( Facility__c ), ISCHANGED( Type__c )  ) &amp;&amp; NOT(ISCHANGED( Last_Sync_Date__c )) &amp;&amp;  (  ISPICKVAL(Assembly_Item_Specification__r.Item__r.Source__c,&apos;Build To Order&apos;)), true, false)</errorConditionFormula>
        <errorMessage>You cannot make this change as the work order is for a build to order item</errorMessage>
    </validationRules>
